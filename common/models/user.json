    "termsAccepted": {
      "type": "boolean"
    },
    "u2f": {
      "type": "boolean"
    },
    "phoneVerified": {
      "type": "boolean"
    },
    "advanced": {
      "type": "boolean"
    },
    "userTrustLevel": {
      "type": "string"

{
  "appenders": {
    "console": {
      "type": "console"
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "console"
      ],
      "level": "info"
    }
  },
  "client": {
    "prefix": "C/",
    "level": "all",
    "appenders": [
      {
        "type": "browser-console"
      },
      {
        "type": "ajax",
        "url": "/ng-rt-core/log",
        "threshold": 5,
        "timeout": 2000
      }
    ]
  }
}